[core]
	editor = mate -w
	excludesfile = /Users/oc/.gitignore
	autocrlf = input
	safecrlf = true
[alias]
	a = add
	b = branch
	ci = commit -v
	cia = commit -v -a
	co = checkout
	cp = cherry-pick
	l = log
	ll = log -p
	lm = log master..
	reb = rebase
	st = status
	up = pull --rebase --stat -v
	sreb = svn rebase
	scd = svn dcommit
	rmsub = "!f(){ git rm --cached \"$1\"; rm -r \"$1\"; git config -f .gitmodules --remove-section \"submodule.$1\"; git config -f .git/config --remove-section \"submodule.$1\"; git add .gitmodules; }; f"
[user]
	name = Ole Christian Rynning
	email = oc@rynning.no
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[merge]
	tool = opendiff
